{
  "hosting": {
    "functions": {
      "source": ".output/server"
    },
    "hosting": [
      {
        "site": "activiti-devtools",
        "public": ".output/public",
        "cleanUrls": true,
        "rewrites": [
          {
            "source": "**",
            "function": "server"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}